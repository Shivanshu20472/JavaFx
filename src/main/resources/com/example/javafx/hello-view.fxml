<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="409.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="102.0" prefWidth="182.0" text="Will Hero" textFill="#5c0dc9">
      <font>
         <Font name="Bell MT" size="40.0" />
      </font>
   </Label>

    <Label fx:id="welcomeText" />
    <Button fx:id="button" onAction="#onHelloButtonClick" text="Play" />
</VBox>
